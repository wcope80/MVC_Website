@model IEnumerable<MVC_Website.Models.MaxAttempt>
@{
    ViewBag.Title = "MaxAttemptList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>MaxAttemptList</h2>


<a class="btn btn-small btn-primary" href="@Url.Action("MaxAttemptInput","Fitness")">Add Attempt <i class="icon icon-white icon-plus-sign"></i></a>
<table class="table table-striped">
    <tr>
        <th>@Html.DisplayNameFor(m => m.Exercise)</th>
        <th>@Html.DisplayNameFor(m => m.Weight)</th>
        <th>@Html.DisplayNameFor(m => m.Reps)</th>
        <th>@Html.DisplayNameFor(m => m.CalculatedMax)</th>
        <th>@Html.DisplayNameFor(m => m.MaxDate)</th>
    </tr>
@foreach (var item in Model) {
    <tr>
        <td>@item.Exercise</td>
        <td>@item.Weight</td>
        <td>@item.Reps</td>
        <td>@item.CalculatedMax</td>
        <td>@item.MaxDate</td>
    </tr>


}


</table>